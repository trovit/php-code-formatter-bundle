{
    "name": "trovit/php-code-formatter-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Trovit",
            "email": "github@trovit.com"
        }
    ],
    "description": "Provides a basic system to organize and execute php code formatters",
    "type": "symfony-bundle",
    "require": {
        "symfony/framework-bundle": "~2.3|^3.1",
        "symfony/console": "~2.3|^3.1",
        "symfony/yaml": "~2.3|^3.1",
        "symfony/process": "~2.3|^3.1",
        "trovit/php-code-formatter": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Trovit\\PhpCodeFormatterBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Trovit\\PhpCodeFormatterBundle\\Tests\\Functional\\": "tests/functional",
            "Trovit\\PhpCodeFormatterBundle\\Tests\\Unit\\": "tests/unit"
        }
    },
    "require-dev": {
        "matthiasnoback/symfony-config-test": "^2.0"
    }
}
